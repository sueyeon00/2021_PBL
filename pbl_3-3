#define _CRT_SECURE_NO_WARNINGS
#include <stdio.h>
#include <stdlib.h>

typedef struct {        //나
	int age;
	char name[100];
}element;

int compare(element* a, element* b) {
	if (a->age > b->age) {
		return 1;
	}
	else if (a-> age < b-> age) {
		return -1;
	}
	else {
		return 0;
	}
}

int main(void) {
	int N;
	printf("N의 값 : ");
	scanf("%d", &N);   //N값 입력
	printf("\n(age name)\n");
	element* arr = (element*)malloc(N * sizeof(element));
	for (int i = 0; i < N; i++) {
		scanf("%d %s", &arr[i].age, arr[i].name);
	}
	printf("-------정렬 후-------\n");
	qsort(arr, N, sizeof(element), compare);
	for (int i = 0; i < N; i++) {
		printf("(%d %s)\n", arr[i].age, arr[i].name);
	}
	return 0;
}
